#include <iostream>

using namespace std;

int main() {
    float mark1, mark2, mark3;
    float average;
    string grade;

    
    cout << "Enter the marks for three subjects:" << endl;
    cout << "Subject 1: ";
    cin >> mark1;
    cout << "Subject 2: ";
    cin >> mark2;
    cout << "Subject 3: ";
    cin >> mark3;
    
    average = (mark1 + mark2 + mark3) / 3;

    
    if (average >= 70) {
        grade = "A";
    } else if (average >= 60) {
        grade = "B";
    } else if (average >= 50) {
        grade = "C";
    } else if (average >= 40) {
        grade = "D";
    } else {
        grade = "E(Fail)";
    }

    
    cout << "Average score: " << average << endl;
    cout << "Grade: " << grade << endl;

    return 0;
    }